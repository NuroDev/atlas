{
    "version": 2,
    "name": "atlas",
    "alias": "atlas.nuro.sh",
    "public": true,
    "github": {
        "autoAlias": true
    },
    "routes": [
        {
            "src": "/docs",
            "status": 301,
            "headers": {
                "Location": "https://docs.atlas.nuro.sh"
            }
        },
        {
            "src": "/(.*)",
            "status": 301,
            "headers": {
                "Location": "https://github.com/nurodev/atlas"
            }
        }
    ]
}
