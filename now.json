{
    "version": 2,
    "name": "atlas",
    "alias": "atlasengine.dev",
    "public": true,
    "github": {
        "autoAlias": true
    },
    "routes": [
        {
            "src": "/book",
            "status": 301,
            "headers": {
                "Location": "https://book.atlasengine.dev"
            }
        },
        {
            "src": "/docs",
            "status": 301,
            "headers": {
                "Location": "https://docs.atlasengine.dev/atlas"
            }
        },
        {
            "src": "/(.*)",
            "status": 301,
            "headers": {
                "Location": "https://github.com/nurodev/atlas"
            }
        }
    ]
}
